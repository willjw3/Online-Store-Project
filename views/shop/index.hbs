

<div class="spacer">
</div>
<h2 align="center">All Whiskies</h2>
<div class="row">
  <div class="col-sm-6 col-md-4 col-md-offset-4 col-sm-offset-3">
    {{# if noMessages }}
      <div></div>
      {{else}}
    <div id="success" class="alert alert-success">
      {{ successMsg }}
    </div>
    {{/if}}
  </div>
</div>
{{# each products }}
  <div class="row">
    {{# each this }}
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <img src="{{ this.imagePath }}" alt="{{ this.title }}" class="img-fluid">
        <div class="caption">
          <h3>{{ this.title }}</h3>
          <h6>{{ this.category }}</h6>
          <h5><strong> ${{ this.price }} </strong></h5>
          <p><a href="/cart/{{this._id}}" class="btn btn-success" style="width:50%" role="button"><i class="fas fa-cart-plus"></i> Add to Cart</a></p>
          <p>{{ this.description }}</p>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
{{/each}}

